@*@Html.ActionLink("Apply Now", "Create", "Candidates")*@
@{
    ViewBag.Title = "Home Page";
    <link href="~/Content/Site.css" rel="stylesheet" />
}

<link href="~/Content/Site.css" rel="stylesheet" />
@Styles.Render("~/Content/css")
@Scripts.Render("~/bundles/modernizr")
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.2/css/bootstrap.min.css">
    <title>Slideshow Example</title>
</head>
<body style="background-color: #232526 ">
    <main>

        @*---------------slideshow-----------*@


        <section id="slide-show">
            <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-indicators">
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true"
                            aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="https://i0.wp.com/silicontrust.org/wp-content/uploads/2023/04/eviden.jpg?resize=800%2C340&ssl=1" class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 id="slide-head-title">Hello, World!</h5>
                            <p id="slide-head-para">Say hello to the best way to kickstart your career in tech.</p>
                            <p>
                                <a href="/Candidates/Create" class="btn btn-primary">Apply Now</a>
                                <a href="/Home/login" class="btn btn-secondary">Interviewer</a>
                            </p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="https://pbs.twimg.com/media/FvGbFiDWcAAZRhV.jpg" class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <!-- Placeholder content for the second slide -->
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="https://eviden.com/wp-content/uploads/2023/04/about-us-video.png" class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <!-- Placeholder content for the third slide -->
                        </div>
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>
        </section>



        @*-------------------------*@




        <br /><br />




        
        <div class="row justify-content-center">
            <div class="col-lg-4">
                <img class="rounded-circle" src="data:image/jpeg;base64," />
                <h2 style="color:brown">Philippe Oliva</h2>
                <p style="color:#808080">Co-CEO and CEO Eviden Business at Atos Group.</p>
            </div>
            <div class="col-lg-4">
                <img class="rounded-circle" src="data:image/jpeg;base64," />
                <h2 style="color:brown">Rakesh Khanna</h2>
                <p style="color:#808080">Chief Executive Officer - Eviden Digital.</p>
            </div>
            <div class="col-lg-4">
                <img class="rounded-circle" src="data:image/jpeg;base64," />
                <h2 style="color:brown">Vasco Gomes</h2>
                <p style="color:#808080">Global CTO - CyberSecurity Products - Eviden.</p>
            </div>
        </div>


        <hr />
        <hr />

        <!-- START THE FEATURETTES -->

        <hr class="featurette-divider">

        <div class="row featurette">
            <div class="col-md-7">
                <h2 class="featurette-heading" style="color:brown">Cloud <span class="text-muted" style="color:mediumpurple">Prepare for the journey to the future landscape.</span></h2>
                <p class="lead" style="color: #999999 ">Achieve the promise of cloud through accelerated advisory, adoption and continuous improvement. We design with greener and more secure architectures. And we deliver through an agile and inclusive culture to enable innovative outcomes..</p>
            </div>
            <div class="col-md-5">
                <img src="https://yourtechdiet.com/wp-content/uploads/2021/10/Cloud-Transformation-what-you-need-to-know.jpg" style="height: 200px; width: 500px; border-radius: 30px"> />
                @*<img class="featurette-image img-fluid mx-auto" data-src="https://" alt="Generic placeholder image">*@
            </div>
        </div>

        <hr class="featurette-divider">

        <div class="row featurette">
            <div class="col-md-7 order-md-2">
                <h2 class="featurette-heading" style="color:brown">Smart Platforms <span class="text-muted" style="color:mediumpurple">Deploy next-generation applications to deliver agility and reliability</span></h2>
                <p class="lead" style="color:#999999">Leverage advanced platforms, transition to Agile DevSecOps foster innovation, and modernize applications with future-proof architecture that delivers impactful benefits.</p>
            </div>
            <div class="col-md-5 order-md-1">
                <img src="https://www.ceatec.com/2021/ja/conference/images/img_category02.jpg" style="height: 200px; width: 500px; border-radius: 30px"> />
                @*<img class="featurette-image img-fluid mx-auto" data-src="holder.js/500x500/auto" alt="Generic placeholder image">*@
            </div>
        </div>

        <hr class="featurette-divider">

        <div class="row featurette">
            <div class="col-md-7">
                <h2 class="featurette-heading" style="color:brown">Transformation Acceleration <span class="text-muted" style="color:mediumpurple">Orchestrate data & technologies to enable new business models</span></h2>
                <p class="lead" style="color: #999999 ">From business transformation through data analytics & AI, Internet of Things to digital business operations, we accelerate your innovation and deliver value to your customers.</p>
            </div>
            <div class="col-md-5">
                <img src="https://www.globalbankingandfinance.com/wp-content/uploads/2022/07/iStock-1391408732.jpg" style="height: 200px; width: 500px;border-radius:30px"> />
                @*<img class="featurette-image img-fluid mx-auto" data-src="holder.js/500x500/auto" alt="Generic placeholder image">*@
            </div>
        </div>

        <hr class="featurette-divider">


    </main>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.2/js/bootstrap.bundle.min.js"></script>


















</body>